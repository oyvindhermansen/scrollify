{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "template-curly-spacing": ["error", "always"],
      "comma-dangle": 0,
      "no-unneeded-ternary": 0,
      "valid-typeof": 0
  }
}
